<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="logistics.work.models.dao.WorkScheduleDao">
    <insert id="add" parameterType="logistics.work.models.domain.WorkSchedule">
        insert into work_schedule(user_id)
        values
        <foreach collection="list" item="workSchedule" separator=",">
            (#{userId})
        </foreach>
    </insert>
</mapper>
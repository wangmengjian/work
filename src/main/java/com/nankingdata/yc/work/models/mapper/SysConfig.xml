<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nankingdata.yc.work.models.dao.SysConfigDao">
    <select id="querySysConfigListByType" parameterType="String" resultType="com.nankingdata.yc.work.models.domain.SysConfig">
        select *
        from sys_config
        where `type`=#{type}
    </select>
</mapper>